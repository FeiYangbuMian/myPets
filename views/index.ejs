    </div>
</div>
<div class="container">
    <div class="row" id="plates">

    </div>
    <footer>

    </footer>
</div>
<script type="text/template" id="template" >
    <div class="col-sm-6 col-md-4"  >
        <div class="thumbnail" id="{{plateId}}">
            <img src="/image/{{platePhoto}}" alt="...">
            <div class="caption">
                <h3>{{plateName}}</h3>
                <p>{{plateIntro}}</p>
            </div>
        </div>
    </div>
</script>
<script src="/js/jquery.js" type="text/javascript" charset="utf-8"></script>
